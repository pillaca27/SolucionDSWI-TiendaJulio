
@model IEnumerable<DSWI_TiendaJulio.Models.Empleado>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.COD_EMP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NOMBRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DNI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TELEFONO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIRECCION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COD_DIS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COD_CARGO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idusuario)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.COD_EMP, "Select", new { id = item.COD_EMP })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOMBRE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DNI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TELEFONO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DIRECCION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.COD_DIS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.COD_CARGO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idusuario)
            </td>
            <td>
                @Html.ActionLink("Selecciona", "Select",
                    new { id = item.COD_EMP }, new { @class = "btn btn-default" })
            </td>
        </tr>
    }

</table>
